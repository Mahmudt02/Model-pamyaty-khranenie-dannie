#include <iostream>
#include <utility>

void swap(int& x, int& y) {

    int temp = x;

    x = y;

    y = temp;
}

int main(int argc, char** argv) {
    int a = 4, b = 9;

    std::cout << "a = " << a << ", b = " << b << std::endl;

    
    swap(a, b);

    std::cout << "a = " << a << ", b = " << b << std::endl;

    return 0;
}
